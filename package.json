{
    "name": "ollama-proxy",
    "version": "1.0.0",
    "description": "Proxy server for Ollama API",
    "main": "dist/index.js",
    "scripts": {
      "build": "tsc",
      "start": "node dist/index.js",
      "dev": "tsx src/index.ts",
      "lint": "eslint src/**/*.ts",
      "test": "echo \"Error: no test specified\" && exit 1",
      "start:ollama": "docker-compose up -d",
      "stop:ollama": "docker-compose down",
      "logs:ollama": "docker-compose logs -f",
      "restart:ollama": "docker-compose restart"
    },
    "keywords": ["ollama", "proxy", "api", "docker"],
    "author": "",
    "license": "MIT",
    "dependencies": {
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "node-fetch": "^3.3.2"
    },
    "devDependencies": {
      "@types/express": "^4.17.17",
      "@types/node": "^20.5.0",
      "eslint": "^8.47.0",
      "@typescript-eslint/eslint-plugin": "^6.4.0",
      "@typescript-eslint/parser": "^6.4.0",
      "ts-node": "^10.9.1",
      "tsx": "^4.7.0",
      "typescript": "^5.1.6"
    },
    "type": "module"
  }